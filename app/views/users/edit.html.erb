<%= form_for @user, url:"/users/#{@user.id}", html: {method: :patch} do |f|%>
    <h1>編集画面</h1>
    <p>お名前：<%= f.text_field :name%></p>
    <p>　年齢：<%= f.text_field :age%></p>
    <input type="submit" value="送信">
<% end %>
<p>
  <%= link_to "登録者一覧", "/users/index"%>
</p>
<p>
  <%= link_to "新規登録", "/users/new"%>
</p>
